    <dependency org="org.hamcrest" name="hamcrest-library" rev="1.3" conf="develop->master(*)" />
    <dependency org="org.assertj" name="assertj-core" rev="1.7.0" conf="develop->master(*)" />
    <dependency org="org.mockito" name="mockito-core" rev="1.10.8" conf="develop->master(*)" />
    <dependency org="org.objenesis" name="objenesis" rev="2.1" conf="develop->master(*)" />
    <dependency org="pl.pragmatists" name="JUnitParams" rev="1.0.3" conf="develop->master(*)" />

  </dependencies>

</ivy-module>
